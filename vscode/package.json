{
  "name": "connector",
  "publisher": "mintlify",
  "displayName": "Mintlify",
  "version": "0.1.27",
  "icon": "assets/logo.png",
  "galleryBanner": {
    "color": "#041811",
    "theme": "dark"
  },
  "repository": {
    "url": "https://github.com/mintlify/connect"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "module": "es2015",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:primary",
    "onCommand:mintlify.link-code",
    "onCommand:mintlify.link-dir"
  ],
  "main": "./dist/extension.js",
  "browser": "./dist/browser/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mintlify.link-code",
        "title": "Link Code to Documentation"
      },
      {
        "command": "mintlify.link-dir",
        "title": "Link folder/file to documentation"
      },
      {
        "command": "mintlify.logout",
        "title": "Logout of Mintlify"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "mintlify.logout",
          "when": "view == primary"
        }
      ],
      "editor/context": [
        {
          "command": "mintlify.link-code",
          "group": "mintlify"
        }
      ],
      "explorer/context": [
        {
          "command": "mintlify.link-dir",
          "group": "mintlify"
        }
      ]
    },
    "views": {
      "primary": [
        {
          "type": "webview",
          "id": "primary",
          "name": "View"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "primary",
          "title": "Mintlify",
          "icon": "assets/icon.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map --config-name extension:node --config-name webviews",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "compile": "webpack --mode development --config-name extension:node --config-name webviews",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/find-parent-dir": "^0.3.0",
    "@types/glob": "^7.2.0",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-tooltip": "^4.2.4",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "axios": "^0.26.1",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.18.0",
    "eslint": "^8.9.0",
    "find-parent-dir": "^0.3.1",
    "fork-ts-checker-webpack-plugin": "^7.2.2",
    "git-rev-2": "^0.1.0",
    "git-url-parse": "^11.6.0",
    "glob": "^7.2.0",
    "ini": "^2.0.0",
    "json5": "^2.2.1",
    "mocha": "^9.2.1",
    "postcss": "^8.4.12",
    "querystring": "^0.2.1",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.1",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.5.0",
    "prepend-http": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-tooltip": "^4.2.21"
  }
}
