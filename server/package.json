  "description": "Backend for Mintlify Connect",
  "main": "index.ts",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "cd ./node_modules/@mintlify/grove/parser && npm run build",
    "start": "ts-node -r tsconfig-paths/register index.ts",
