    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "extract-comments": "^1.1.0",
    "marked": "^4.0.14",
    "mongoose": "^6.0.15",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.14",
    "stripe": "^8.207.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
