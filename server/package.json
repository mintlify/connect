    "lint": "eslint . --ext .ts",
    "ts-node": "ts-node -T -P tsconfig.json -r tsconfig-paths/register",
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mintlify/connect.git"
  },
